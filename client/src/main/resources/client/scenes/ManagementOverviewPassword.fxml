<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="385.0" prefWidth="451.0" stylesheets="@../../stylesheets/main.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.ManagementOverviewPasswordCtrl">
   <children>
      <Label fx:id="inputPasswordLabel" alignment="CENTER" layoutX="32.0" layoutY="138.0" prefHeight="34.0" prefWidth="388.0" styleClass="h1" text="Input password for management overview:">
         <font>
            <Font size="10.0" />
         </font></Label>
      <PasswordField fx:id="passwordField" layoutX="86.0" layoutY="185.0" prefHeight="25.0" prefWidth="190.0" promptText="Enter password..." />
      <Button fx:id="logInButton" layoutX="287.0" layoutY="181.0" mnemonicParsing="false" onAction="#logInCheck" prefHeight="25.0" prefWidth="79.0" styleClass="positiveButton" stylesheets="@../../stylesheets/main.css" text="Log in" />
      <Label fx:id="logInFeedback" layoutX="86.0" layoutY="222.0" prefHeight="25.0" prefWidth="166.0" styleClass="errorText">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Button fx:id="goBackToMainScreen" layoutX="14.0" layoutY="349.0" mnemonicParsing="false" onAction="#goBackToMain" prefHeight="25.0" prefWidth="30.0" styleClass="positiveButton" />
   </children>
</AnchorPane>
